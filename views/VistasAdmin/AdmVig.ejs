					<input name="cap" type="number" id="capacidad">
					<label class="activate" style="color: black" for="capacidad">Capacidad del fraccionamiento</label>
				</div>
				<div class="input-field col s10">
					<div class="input-field col s8"><a class="btn waves-effect waves-light waves-red btb-flat" onclick="InsertarFrac(event)" name="Entrar" style="background-color: #2196f3;">Ingresar</a>
				</div>
			</div>
			</form>
